{
	"name": "listerr",
	"version": "1.0.0",
	"description": "A todo app based on MERN stack",
	"main": "server.js",
	"engines": {
		"node": "^8.11.3",
		"npm": "^6.2.0"
	},
	"scripts": {
		"start": "node server.js",
		"server": "nodemon server.js",
		"client": "npm start --prefix client",
		"dev": "concurrently --kill-others \"npm run server\" \"npm run client\""
	},
	"keywords": [
		"react",
		"redux",
		"node",
		"express",
		"mongoDB",
		"mongoose"
	],
	"author": "Vikas Raj",
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.18.3",
		"express": "^4.16.3",
		"jsonwebtoken": "^8.3.0",
		"mongoose": "^5.2.5",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"concurrently": "^3.6.0",
		"nodemon": "^1.18.3"
	}
}
